{
    "extends": "airbnb",
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        },
    },
    "rules": {
        "comma-dangle": 0,
        "func-names": 0,
        "prefer-rest-params": 0,
        "prefer-arrow-callback": 0,
        "react/prefer-stateless-function": 1,
        "space-before-function-paren": 0,
        "eol-last": 0,
        "indent": [2, 4, {
            "SwitchCase": 1
        }]
    }
}
